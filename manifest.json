{
	"manifest_version": 3,
	"name": "Website Tracker",
	"description": "Tracks websites visited and logs the time of visit.",
	"version": "1.0",
	"permissions": [
	  "history",
	  "tabs",
	  "storage"
	],
	"background": {
	  "service_worker": "background.js"
	},
	"host_permissions": [
	  "<all_urls>"
	],
	"action": {
	  "default_popup": "popup.html",
	  "default_icon": {
		"16": "images/get_started16.png",
		"48": "images/get_started48.png",
		"128": "images/get_started128.png"
	  }
	}
  }
  